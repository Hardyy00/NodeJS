<body>
    <%- include('../partials/navigation.ejs') %>

    <main>
        <% if(products.length >0) { %>

            <ul>

                <% products.forEach(product=> { %>

                    <li>
                        <p><%= product.productData.title %> (<%= product.qty %>)</p>

                        <form action=""></form>
                    </li>

                <% }); %>
            </ul>
         
        <% } else { %>
            
            <h1>No Products in the cart</h1>
            
         <% } %>   
    </main>
</body>

